{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/standard"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-console": "warn"
  },
  "globals": {
    "chrome": true,
    "pluginCheck": true,
    "rootCheckAuth": true
  }
}
